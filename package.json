{
  "devDependencies": {
    "@tanstack/router-plugin": "^1.131.44",
    "@vitejs/plugin-react": "^5.0.2",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.16.2",
    "supabase": "^2.48.3",
    "tsx": "^4.20.5",
    "vite": "^7.1.5"
  },
  "type": "module",
  "scripts": {
    "dev": " NODE_ENV=development next dev",
    "dev:db": "NODE_ENV=development prisma studio",
    "build": "NODE_ENV=production next build",
    "build:dev": "NODE_ENV=development next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:seed": "dotenv -e .env.development -- prisma db seed",
    "db:seed:prod": "dotenv -e .env.production -- node prisma/seed-production.mjs",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate reset --force",
    "db:migrate:deploy": "dotenv -e .env.production -- prisma migrate reset --force",
    "db:push:dev": "dotenv -e .env.development -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:generate": "prisma generate"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@netlify/plugin-nextjs": "^5.13.4",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@rsbuild/plugin-react": "^1.4.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "@tanstack/router-cli": "^1.131.44",
    "@types/node": "24.5.2",
    "@types/react-dom": "^19.1.9",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "jose": "^6.1.0",
    "lint": "^0.8.19",
    "lucide-react": "^0.544.0",
    "next": "^15.5.4",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
